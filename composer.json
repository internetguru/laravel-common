{
    "name": "internetguru/laravel-common",
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "InternetGuru\\LaravelCommon\\": "src/",
            "Tests\\": "tests/"
        },
        "files": [
            "src/Support/macros.php"
        ]
    },
    "authors": [
        {
            "name": "petrzpav",
            "email": "pavel.petrzela@internetguru.cz"
        }
    ],
    "require": {
        "laravel/framework": "^10.0 || ^11.0",
        "torann/geoip": "^3.0"
    },
    "require-dev": {
        "laravel/pint": "^1.17",
        "orchestra/testbench": "^9.5",
        "phpunit/php-code-coverage": "^11.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "InternetGuru\\LaravelCommon\\CommonServiceProvider"
            ]
        }
    }
}
